      <artifactId>drools-compiler</artifactId>
      <version>7.26.0.Final-redhat-00005</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.drools</groupId>
      <artifactId>drools-workbench-models-guided-dtable</artifactId>
      <version>7.26.0.Final-redhat-00005</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.kie</groupId>
      <artifactId>kie-dmn-feel</artifactId>
      <version>7.26.0.Final-redhat-00005</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.kie</groupId>
      <artifactId>kie-dmn-api</artifactId>
      <version>7.26.0.Final-redhat-00005</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.kie</groupId>
      <artifactId>kie-dmn-core</artifactId>
      <version>7.26.0.Final-redhat-00005</version>
      <scope>test</scope>
    </dependency>
    <dependency>
			<groupId>org.kie</groupId>
			<artifactId>kie-dmn-jpmml</artifactId>
			<version>7.27.0.Final</version>
		</dependency>

        <!-- https://mvnrepository.com/artifact/org.jpmml/pmml-evaluator -->
        <dependency>
            <groupId>org.jpmml</groupId>
            <artifactId>pmml-evaluator</artifactId>
            <version>1.4.9</version>
        </dependency>
		<!-- https://mvnrepository.com/artifact/org.jpmml/pmml-evaluator-extension -->
